<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shaopeng.marqueeview">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.shaopeng.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoTitle">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.shaopeng.coord.CoordActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.shaopeng.zoom.ZoomActivity" />
        <activity android:name="com.shaopeng.zoom.PullScrollViewActivity" />
        <activity android:name="com.shaopeng.zoom.PullListViewActivity" />
        <activity android:name="com.shaopeng.zoom.PullGridViewActivity" />
        <activity android:name="com.shaopeng.zoom.PullWebViewActivity" />
        <activity android:name="com.shaopeng.zoom.PullRecyclerViewActivity" />
        <activity android:name="com.shaopeng.zoom.PullViewActivity" />
        <activity android:name="com.shaopeng.zoom.PullViewGroupActivity" />
        <activity android:name="com.shaopeng.dialog.DialogActivity"></activity>
    </application>

</manifest>